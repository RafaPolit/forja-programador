<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/bootstrap.min.css' />
  </head>
  <body>
    <div class="container">
      <h1><%= title %></h1>
      <table class="table table-striped table-bordered table-hover">
        <thead>
          <tr>
            <th>Input</th>
            <th>Tiempo de Sue√±o</th>
            <th>Secuencia de Acciones</th>
            <th>Followers Perdidos</th>
          </tr>
        </thead>
        <tbody>
          <% test_cases.forEach(function(test_case) { %>
            <tr>
              <td><%= test_case.baby_cries_string %></td>
              <td><%= test_case.programmer.sleep_time_str %></td>
              <td><%= test_case.programmer.actions_str %></td>
              <td><%= test_case.followers.lost_followers %></td>
            </tr>
          <% }); %>
        </tbody>
      </table>
    </div>
  </body>
</html>